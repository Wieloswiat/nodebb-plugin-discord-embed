{
    "id": "nodebb-plugin-discord-embed",
    "url": "https://github.com/Wieloswiat/nodebb-plugin-discord-embed",
    "library": "./library.js",
    "hooks": [
        {
            "hook": "static:app.load",
            "method": "init"
        },
        {
            "hook": "filter:widgets.getWidgets",
            "method": "getWidgets"
        },
        {
            "hook": "filter:widget.render:discord-chat",
            "method": "renderDiscordWidget"
        }
    ],
    "staticDirs": {
        "static": "./static"
    },
    "less": ["static/style.less"],
    "templates": "static/templates"
}
